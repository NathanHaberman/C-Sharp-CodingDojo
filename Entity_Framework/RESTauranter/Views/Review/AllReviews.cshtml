@model RESTauranter.Models.Review

<h1 class="mt-3">All Reviews</h1>

@{
    <hr>
    foreach(var review in (dynamic)(@TempData["AllReviews"]))
    {
        <div class="p-2">
            <h3>@review.name</h3>
            <p>Reviewed by: @review.reviewer</p>
            <p>Stars: @review.stars</p>
            <p>Visited: @review.visitDate</p>
            <p>@review.theMeat</p>
        </div>
        <hr>
    }
}